    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-smells": "^1.0.1"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "conventional-changelog": "^1.1.3",
    "cz-conventional-changelog": "^2.0.0",
    "semantic-release": "^6.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastienbarre/eslint-config.git"
