{
  "name": "@sebastienbarre/eslint-config",
  "description": "Personal ESLint config, with dependencies",
  "license": "BSD-3-Clause",
  "author": {
    "name": "Sebastien Barre",
    "email": "sebastien.barre@gmail.com",
    "url": "http://barre.io"
  },
  "bin": {
    "eslint": "bin/lint.js"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "commit": "git-cz",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "local": "yarn unlink && yarn link"
  },
  "engines": {
    "node": ">=0.11.x"
  },
  "dependencies": {
    "babel-eslint": "^7.x",
    "eslint": "^3.x",
    "eslint-config-airbnb": "^14.x",
    "eslint-config-prettier": "^1.x",
    "eslint-plugin-flowtype": "^2.x",
    "eslint-plugin-import": "^2.x",
    "eslint-plugin-jsdoc": "^3.x",
    "eslint-plugin-jsx-a11y": "^4.x",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^6.x",
    "eslint-plugin-smells": "^1.x",
    "prettier": "^1.1.0"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "conventional-changelog": "^1.1.3",
    "cz-conventional-changelog": "^2.0.0",
    "semantic-release": "^6.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastienbarre/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/sebastienbarre/eslint-config/issues",
    "email": "sebastien.barre@gmail.com"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ]
}
