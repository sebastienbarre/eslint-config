{
  "name": "@sebastienbarre/eslint-config",
  "description": "Personal ESLint config, with dependencies",
  "license": "BSD-3-Clause",
  "author": {
    "name": "Sebastien Barre",
    "email": "sebastien.barre@gmail.com",
    "url": "http://barre.io"
  },
  "bin": {
    "eslint": "bin/lint.js"
  },
  "config": {
    "commitizen": {
